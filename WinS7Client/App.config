<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  
  <connectionStrings>
    <add name="PLC0" connectionString="Server=127.0.0.1; Rack = 0; Slot = 2;"/>
    <add name="PLC1" connectionString="Server=192.168.157.101; Rack = 0; Slot = 2;"/>
    <add name="PLC2" connectionString="Server=192.168.157.102; Rack = 0; Slot = 2;"/>
    <add name="PLC3" connectionString="Server=192.168.157.103; Rack = 0; Slot = 1;"/>
    <add name="PLC4" connectionString="Server=192.168.157.104; Rack = 0; Slot = 1;"/>
    <add name="PLC5" connectionString="Server=192.168.157.105; Rack = 0; Slot = 1;"/>
    <add name="PLC6" connectionString="Server=192.168.157.106; Rack = 0; Slot = 1;"/>
    <add name="PLC7" connectionString="Server=192.168.157.107; Rack = 0; Slot = 1;"/>
    <add name="PLC8" connectionString="Server=192.168.157.108; Rack = 0; Slot = 1;"/>
    <add name="PLC9" connectionString="Server=192.168.157.109; Rack = 0; Slot = 1;"/>
    <add name="PLC10" connectionString="Server=192.168.157.110; Rack = 0; Slot = 1;"/>
    <!-- your comment -->
  </connectionStrings>
  

  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="console" />
      <appender-ref ref="file" />
    </root>
    <appender name="console" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="file" type="log4net.Appender.RollingFileAppender">
      <file value="WinS7ClientLogger.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="1MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline" />
      </layout>
    </appender>
  </log4net>
  
  <startup>
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>

</configuration>